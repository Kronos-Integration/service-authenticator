{
  "name": "@kronos-integration/service-authenticator",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/service-authenticator.mjs",
  "exports": {
    ".": "./src/service-authenticator.mjs"
  },
  "description": "authentication providing service",
  "keywords": [
    "jwt",
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/service-authenticator.mjs --section=API",
    "lint": "documentation lint ./src/service-authenticator.mjs"
  },
  "dependencies": {
    "@kronos-integration/endpoint": "^9.2.8",
    "@kronos-integration/service": "^9.2.13",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.1",
    "documentation": "^13.0.2",
    "semantic-release": "^17.1.2"
  },
  "engines": {
    "node": ">=14.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-authenticator.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-authenticator/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-authenticator#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
